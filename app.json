{
  "expo": {
    "name": "cargoTestApp",
    "slug": "cargoTestApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/transport.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#008EFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/transport.png",
        "backgroundColor": "#008EFF"
      },
      "package": "com.eldarkns.transportApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
